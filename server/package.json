{
  "name": "anoda-facematch",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node -r module-alias/register src/index.js",
    "dev": "nodemon -r module-alias/register src/index.js --ignore logQueue.json | pino-pretty -c",
    "worker": "node -r module-alias/register src/worker.js",
    "worker:dev": "nodemon -r module-alias/register src/worker.js | pino-pretty -c",
    "email": "node -r module-alias/register src/email.js",
    "email:dev": "nodemon -r module-alias/register src/email.js | pino-pretty -c",
    "create-model": "node src/utils/scripts/index.script.js model",
    "create-handler": "node src/utils/scripts/index.script.js handler",
    "create-middleware": "node src/utils/scripts/index.script.js middleware",
    "create-service": "node src/utils/scripts/index.script.js service",
    "test": "NODE_ENV=test node src/utils/scripts/test/execute.js",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "testinit": "NODE_ENV=test node src/utils/scripts/test/initialize.js"
  },
  "_moduleAliases": {
    "@config": "src/config",
    "@utils": "src/utils",
    "@models": "src/models",
    "@routes": "src/routes",
    "@middlewares": "src/routes/middlewares",
    "@services": "src/services",
    "@queue": "src/queue",
    "@email": "src/email"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "colorette": "^2.0.20",
    "commander": "^13.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "ioredis": "^5.5.0",
    "jest": "^29.7.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.2",
    "pg": "^8.13.1",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "rate-limit-redis": "^4.2.0",
    "sharp": "^0.33.5",
    "supertest": "^7.1.0",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "jest-module-name-mapper": "^0.1.5",
    "prisma": "^6.6.0"
  }
}
